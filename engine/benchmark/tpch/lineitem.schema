create table lineitem (
	l_orderkey int,
	l_partkey int,
	l_suppkey int,
	l_linenumber int,
	l_quantity float,
	l_extendedprice float,
	l_discount float,
	l_tax float,
	l_returnflag char,
	l_linestatus char,
	l_shipdate date,
	l_commitdate date,
	l_receiptdate date,
	l_shipinstruct string,
	l_shipmode string,
	l_comment string
	/* primary key (l_orderkey, l_linenumber) */
) 
